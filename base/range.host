

fn range(start stop? step?)
    var nums list!
    var args : unshift _args 'i
    eval
    	` for args
    		' push nums i
    return nums

;;;
fn range(start stop? step?)
    var nums list!
    var args : unshift _args `i
    apply for (list args (' ` push nums i))
    return nums
;;;

export range


range 1 === (, 0 1)

range 1 4 === (, 1 2 3 4)

range 2 6 2 === (, 2 4 6)

range 3 0 -1 === (, 3 2 1 0)

range 1 -5 2 === list!