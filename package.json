{
  "name": "hostlang",
  "version": "0.0.8",
  "description": "A metalanguage for programming",
  "main": "hostlang.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node hostlang.js -e 'runTests! >> log'",
    "start": "node hostlang.js repl",
    "build": "node hoststrap.js && webpack && echo 'var hostlang = ' | cat - hostlang.bundle.js > temp && mv temp hostlang.bundle.js && cp hostlang.bundle.js ../hostlang.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mark-archer/hostlang.git"
  },
  "keywords": [
    "host",
    "meta",
    "lang",
    "language",
    "lisp"
  ],
  "author": "Mark Archer",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mark-archer/hostlang/issues"
  },
  "homepage": "https://github.com/mark-archer/hostlang#readme",
  "dependencies": {
    "cookies": "^0.7.0",
    "lodash": "^4.17.4",
    "underscore": "^1.8.3",
    "uuid": "^3.0.1"
  }
}
