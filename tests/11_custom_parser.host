;; test adding and removing a parser
fn parseHI(pi)
	var nw pi.nextWord
	set nw nw!
	if(== nw "HI")
		set pi.i (+ pi.i 2)
		var el pi.endList
		el!
		push (pi.clist) : 'BYE
		var nl pi.newList
		nl!
		return true
	false

push parsers parseHI

parse "HI"
=== (' BYE)

remove parsers parseHI

parse "HI"
=== (' HI)
